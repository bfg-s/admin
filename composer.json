{
    "name": "bfg/admin",
    "description": "Laravel Admin by BFG",
    "type": "bfg-library",
    "keywords": [
        "laravel",
        "admin",
        "lte",
        "bfg",
        "wood"
    ],
    "homepage": "https://packagist.org/packages/bfg/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "bfg",
            "email": "bfg.script@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1",
        "ext-pdo": "*",
        "bfg/entity": "*",
        "bfg/repository": "^1.0",
        "bfg/embedded-call": "^1.0",
        "maatwebsite/excel": "^3.1.37",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "composer/composer": "^1.9|^2.0",
        "symfony/dom-crawler": "^v6.0.1",
        "symfony/css-selector": "^v6.0.1|^v7.0.0",
        "almasaeed2010/adminlte": "^3.1.0",
        "barryvdh/laravel-ide-helper": "^v2.13.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/google2fa-qrcode": "*",
        "stichoza/google-translate-php": "^v5.1.2",
        "intervention/image": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.9",
        "laravel/dusk": "^v8.1.0"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admin\\ServiceProvider"
            ],
            "aliases": {
                "Admin": "Admin\\Facades\\AdminFacade",
                "Navigate": "Admin\\Facades\\NavigateFacade"
            }
        }
    }
}
